// clang-format off
{
  "configurations": [
    {
      "name": "Metron",
      "preLaunchTask": "build_metron_bin",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/metron",
      "cwd": "${workspaceFolder}",
      //"args" : [      "-d", "-v", "-e", "-c", "examples/scratch.h", "-o", "examples/scratch.sv"],
      "args" : [
        "-d",
        "-v",
        "-e",
        "-i", "../pinwheel",
        //"-c", "examples/scratch.h",
        //"-c", "tests/metron_good/structs_as_args.h",
        //"-c", "examples/uart/metron/uart_hello.h",
        //"-c", "examples/rvsimple/metron/config.h",
        //"-c", "pinwheel/rtl/pinwheel_core.h",
        //"-c", "pinwheel/rtl/regfile.h",
        "-c", "examples/gb_spu/metron/MetroBoySPU2.h",
        "-o", "examples/scratch.sv"
      ],
      "setupCommands": [
        { // Display content in STL containers pretty
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
// clang-format on
